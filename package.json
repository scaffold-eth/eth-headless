{
  "name": "eth-headless",
  "version": "0.1.0",
  "scripts": {
    "format": "prettier --write src/**/*.ts{,x}",
    "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "clean": "shx rm -rf ./lib",
    "build": "yarn clean && node esbuild.config.mjs && ts-patch i -s && tsc -p tsconfig.build.json",
    "package": "shx cp -R package.json ./lib && shx cp -R README.md ./lib",
    "publish:yalc": "yarn build && yarn package && cd lib && yalc publish --no-script --push"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.1.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/node": "^16.11.29",
    "@types/react": "^18.0.7",
    "@types/react-dom": "^18.0.0",
    "eth-hooks": "^4.0.48",
    "ethers": "^5.6.4",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.6.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "prettier": "^2.6.2",
    "prettier-eslint": "^14.0.1",
    "webpack": "^5.72.0",
    "shx": "^0.3.4",
    "esbuild": "^0.14.38",
    "esbuild-node-externals": "^1.4.1",
    "tsconfig-paths": "^3.14.1",
    "ts-patch": "^2.0.1",
    "typescript-transform-paths": "^3.3.1"
  },
  "license": "MIT"
}
